{
    "builds": [
        {
          "src": "ToDoApp/wsgi.py",
         
          "use": "@ardnt/vercel-python-wsgi",
          "config": { "maxLambdaSize": "15mb" }
        }
      ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "ToDoApp/wsgi.py"
        }
      ]
    }
    